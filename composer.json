{
  "name": "ersalak/ersalak-laravel-sms",
  "description": "A modern Laravel SMS client for sending and reporting messages via REST API",
  "type": "laravel-package",
  "homepage": "https://github.com/RezaKarimpour/ersalak-laravel-sms",
  "keywords": [
    "laravel",
    "Ersalak",
    "sms",
    "api"
  ],
  "version": "1.0.0",
  "authors": [
    {
      "name": "Ersalak Team",
      "email": "info@ersalak.ir"
    },
    {
      "name": "Reza Karimpour",
      "email": "rezakarimpour.job@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0 || ^8.1 || ^8.2",
    "guzzlehttp/guzzle": "^7.0",
    "psr/log": "^1.1 || ^2.0 || ^3.0",
    "illuminate/support": "^9.0 || ^10.0 || ^11.0 || ^12.0"
  },
  "autoload": {
    "psr-4": {
      "Ersalak\\Sms\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Ersalak\\Sms\\SmsServiceProvider"
      ],
      "aliases": {
        "ErsalakSms": "Ersalak\\Sms\\Facade\\ErsalakSmsFacade"
      }
    }
  },
  "minimum-stability": "stable",
  "license": "MIT"
}
